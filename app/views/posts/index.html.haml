- @posts.each do |p|
  .row
    .col-md-1
    .col-md-10.image-container.box-container
      =image_tag p.photo.url 
      %br
      %br
      %p.title= p.title 
      %p.description
        = p.description
      %p.info Visited on: #{p.visited_on}
      %p.info Place: #{p.place + ', ' + p.country}
      %p.info Author: Rishi Jain 
    .col-md-1
  %br
